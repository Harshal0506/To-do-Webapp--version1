<%- include('header')%>
	<div class="box" id="heading">
	<h1> <%=listTitle%></h1>
	</div>

	<div class="box">
		
		
		<% for(var i=0;i<listItems.length ;i++){ %>
		<div class="item">
			<input type="checkbox" >
		<p> 
			<%= listItems[i] %>
				
		</p>
		</div>

	<% }%>
	
	
	

	<form class="item" method="POST" action="/">
		<input autocomplete="off" type="text" name="newItem" placeholder="New Item" required>
		<button type="submit" name="button" value= <%=listTitle%> >+</button> 
	</form>
	</div>

<%- include('footer');  %>